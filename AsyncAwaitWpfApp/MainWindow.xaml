<Window x:Class="AsyncAwaitWpfApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AsyncAwaitWpfApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <Button Click="Start_Click" Content="Start"></Button>
        <Button Click="StartAsync_Click" Content="Start Async"></Button>
        <Button Click="RequestAsync_Click" Content="Start Request"></Button>
        <TextBlock x:Name="Info" />
    </StackPanel>
</Window>
